                                </numIndex>
                                <numIndex index="1">none</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">
                                    LLL:EXT:bibsonomy_csl/Resources/Private/Language/locallang_be.xlf:flexform.custom.personLink.mode.modal
                                </numIndex>
                                <numIndex index="1">modal</numIndex>
